{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Quartz": "Information"
    }
  },
  "AllowedHosts": "*",
  "Quartz": {
    "TablePrefix": "quartz.qrtz_",
    "PerformSchemaValidation": true,
    "WaitForJobsToComplete": true,
    "MaxConcurrency": 10,
    "MisfireThresholdSeconds": 60,
    "EnableClustering": false,
    "ClusterCheckinIntervalSeconds": 20,
    "ClusterMisfireThresholdSeconds": 30
  }
}
